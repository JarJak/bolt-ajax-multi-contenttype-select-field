{
    "name": "soapbox/ajaxmultictselectfield",
    "description": "An extension to add an AJAX select field which can handle multiple content types.",
    "type": "bolt-extension",
    "keywords": [
        "select field",
        "multiple select",
        "ajax select"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Hunt",
            "email": "robert.hunt@soapbox.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Soapbox\\AjaxMultiCTSelectField\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Soapbox\\AjaxMultiCTSelectField\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Soapbox\\AjaxMultiCTSelectField\\AjaxMultiCTSelectFieldExtension"
    }
}
